import pandas as pd
import streamlit as st
import numpy as np
st.title("LOL surveys")

st.header("personal")
gender = st.radio("gender(성별)", ("male(남)", "female(여)"))

age = st.number_input("Enter the your age(나이)", 1)

st.header("IN game")
champ = st.text_input("your most champione(모스트 챔프)")

position = st.radio("your position line(라인)", ("top", "jg", "mid", "ad","sup"))

st.subheader("using role group(자주하는 역할군)")
checkbox_btn = st.checkbox('Slayer(암살자)')
checkbox2_btn = st.checkbox('Fighter(전사)')
checkbox3_btn = st.checkbox('Mage(마법사)')
checkbox4_btn = st.checkbox('Marksman(원거리 딜러)')
checkbox5_btn = st.checkbox('Controller(서포터)')
checkbox6_btn = st.checkbox('Tank(탱커)')

st.time_input("one day on average play Time(하루 평균 플레이 시간)")

